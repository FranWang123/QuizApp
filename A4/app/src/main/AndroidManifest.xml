<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.uwaterloo.cs349.a349">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TopicSelect"
            android:label="Topic Selection"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Question_1"
            android:label="Question"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Question_2"
            android:label="Question"
            android:parentActivityName=".Question_1" />
        <activity
            android:name=".Question_3"
            android:label="Question"
            android:parentActivityName=".Question_2" />
        <activity
            android:name=".Question_4"
            android:label="Question"
            android:parentActivityName=".Question_3"/>
        <activity
            android:name=".Question_5"
            android:label="Question"
            android:parentActivityName=".Question_4"/>
        <activity
            android:name=".FinalScore"
            android:label="Results"
            android:parentActivityName=".TopicSelect" />
    </application>

</manifest>